{
  "name": "EF-PRIME-MD",
  "description": "A bot powered by Optimus prime theme known as ef-prime",
  "logo": "https://files.catbox.moe/hc9hh3.jpg",
  "keywords": ["EF-PRIME-MD"],
  "repository": "https://github.com/efkidgamerdev/EF-PRIME-MD",
  "stack": "container",
  "env": { 
      "SESSION_ID": {
      "description": "your session id",
      "required": true,
      "value": ""
    },
    "MODE": {
      "description": "mode public or self",
      "required": true,
      "value": "private"
    },
    "ANTI_DELETE": {
      "description": "make it true for use anti delete",
      "required": true,
      "value": "true"
    },
  "AUTO_STATUS_SEEN": {
      "description": "make it true if you want bot to view status",
      "required": false,
      "value": "true"
    },
    "AUTO_STATUS_REPLY": {
      "description": "make it true if you want to send message after seen status",
      "required": false,
      "value": "false"
    },
    "STATUS_READ_MSG": {
      "description": "set your status reply message",
      "required": false,
      "value": "*Status seen by EF-PRIME-MD *"
    },
    "OWNER_NAME": {
      "description": "set your status reply message",
      "required": false,
      "value": "Frank kaumba dev"
    },
    "OWNER_NUMBER": {
      "description": "set your status reply message",
      "required": true,
      "value": "967714585737"
    },
    "AUTO_READ": {
      "description": "make it true if you want bot to read message",
      "required": false,
      "value": "false"
    },
    "AUTO_TYPING": {
      "description": "make it true if you want bot to show typing",
      "required": false,
      "value": "false"
    },
    "AUTO_RECORDING": {
      "description": "make it true if you want bot to show recording audio",
      "required": false,
      "value": "false"
    },
    "ALWAYS_ONLINE": {
      "description": "make it true if you want bot to show always online",
      "required": false,
      "value": "false"
    },
    "AUTO_BLOCK": {
      "description": "make it true if you want bot to auto block only 265 numbers",
      "required": false,
      "value": "false"
    },
    "REJECT_CALL": {
      "description": "make it true if you want to reject call",
      "required": false,
      "value": "false"
    },
    "PREFIX": {
      "description": "Your Bot Prefix default (.)",
      "required": false,
      "value": "."
    }
    },
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}

